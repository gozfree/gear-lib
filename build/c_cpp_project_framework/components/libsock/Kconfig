config LIBSOCK_ENABLED
    bool "Enable libsock"
    default n

config LIBSOCK_ENABLED
    bool "Enable libsock"
    default n
    if LIBSOCK_ENABLED
        config ENABLE_SOCK_EXT
            bool "Enable sock ext"
            default y
            depends on LIBPOSIX_ENABLED
            depends on LIBGEVENT_ENABLED
            depends on LIBTHREAD_ENABLED
        config ENABLE_PTCP
            bool "Enable ptcp"
            default n
            depends on LIBPTCP_ENABLED
    endif

